package BLM::PDF::Render::Newline;

use BLM::PDF::Log;
use strict;

sub new
{
   my ($class, $drop) = @_;
   $drop = 0 unless defined $drop;
   return bless \$drop, $class;
} # sub new

sub setDrop
{
   my $this = shift;
   $$this = shift;
} # sub setDrop

sub getDrop
{
   my $this = shift;
   return $$this;
} # sub getDrop

1;


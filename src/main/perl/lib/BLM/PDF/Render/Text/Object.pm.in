package BLM::PDF::Render::Text::Object;

use BLM::PDF::Log;
use strict;

sub new
{
   bless \my $this, shift;
} # sub new

sub addBytes
{
   my $this = shift;
   $$this .= shift;
} # sub addBytes

sub render
{
   my $this = shift;
   $$this;
} # sub render


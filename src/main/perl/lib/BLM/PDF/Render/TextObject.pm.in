package BLM::PDF::Render::TextObject;

use BLM::PDF::Log;
use strict;

sub new
{
   my $class = shift;

   return bless \my $this, $class;
} # sub new

sub addBytes
{
   my $this = shift;
   $$this .= shift;
} # sub addBytes

sub render
{
   my $this = shift;
   return $$this;
} # sub render

1;


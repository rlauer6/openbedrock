package BLM::PDF::Text::Text;

require 5.005;
use strict;

sub new
{
   my $class = shift;

   my $this = { 'properties'	=> shift,
		'text' 		=> shift,
              };

   bless $this, $class;

   return($this);
} # sub new

sub getProperties
{
   my $this = shift;
   return $$this{'properties'};
} # sub setFont

sub getText
{
   my $this = shift;
   return $$this{'text'};
} # sub setFont

# Text.pm

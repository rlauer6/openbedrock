package BLM::PDF::Text::Text;

require 5.005;
use strict;

sub new
{
   my $class = shift;

   my $this = bless { 'properties'	=> shift,
	              'text' 		=> shift,
                    };

   bless $this, $class;
} # sub new

sub getProperties { ${_[0]}->{'properties'}; }
sub getText	  { ${_[0]}->{'text'}; }

1;


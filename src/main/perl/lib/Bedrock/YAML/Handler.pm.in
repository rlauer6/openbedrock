package Bedrock::YAML::Handler;

use strict;
use warnings;

use parent qw/Bedrock::Serializer/;

use Role::Tiny::With;
with 'Bedrock::Serializer';

use YAML::Syck;
use Scalar::Util qw/reftype/;

sub deserialize {
  my $self = shift;

  return Load(shift);
}

sub serialize {
  my $self = shift;

  return Dump(shift);
}

1;

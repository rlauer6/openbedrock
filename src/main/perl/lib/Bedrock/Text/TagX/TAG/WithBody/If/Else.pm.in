#!/usr/bin/perl -w

# ident "@(#) $Header$"
# ident "@(#)        $Name$"

package TagX::TAG::WithBody::If::Else;

use strict;

BEGIN
{
    use Exporter ();
    use vars	qw ($VERSION @ISA);
    $VERSION    = do { my @r = (q$Revision$ =~ /\d+/g); sprintf "%d."."%02d" x $#r, @r };
    @ISA	= qw (Exporter);
}

use TagX::TAG::WithBody::If 1.09;

@ISA = qw (TagX::TAG::WithBody::If);

sub parent_if
{
    my $self = shift;
    $self->{'parent_if'} = shift if @_ == 1;
    $self->{'parent_if'};
}

sub end_if
{
    my $self = shift;
    $self->parent_if->parent;
}


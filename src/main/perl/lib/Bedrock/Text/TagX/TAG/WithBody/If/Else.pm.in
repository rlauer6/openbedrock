package TagX::TAG::WithBody::If::Else;

use strict;
use vars qw (@ISA);
use TagX::TAG::WithBody::If;
@ISA = qw (TagX::TAG::WithBody::If);

sub parent_if
{
    my $self = shift;
    $self->{'parent_if'} = shift if @_ == 1;
    $self->{'parent_if'};
}

sub end_if
{
    my $self = shift;
    $self->parent_if->parent;
}


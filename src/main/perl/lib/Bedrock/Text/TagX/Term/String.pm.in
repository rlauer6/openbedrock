package TagX::Term::String;

use strict;

BEGIN
{
    use Exporter ();
    use vars	qw ($VERSION @ISA);
    $VERSION    = do { my @r = (q$Revision$ =~ /\d+/g); sprintf "%d."."%02d" x $#r, @r };
    @ISA	= qw (Exporter);
}

use TagX::Term 1.10;

@ISA = qw (TagX::Term);

sub eval
{
    my $self = shift;
    $$self;
}

1;


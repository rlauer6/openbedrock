package TagX::Term::String;

use strict;
use TagX::Term;

use vars qw (@ISA);

@ISA = qw (TagX::Term);

sub eval
{
    my $self = shift;
    if ( $$self =~ /^\$/ ) {
	$self->resolve_var ($$self, @_);
    } else {
	$$self;
    }
}

1;


#!/usr/bin/perl -w

# ident "@(#) $Header$"
# ident "@(#)        $Name$"

package TagX::Term::String;

use strict;

BEGIN
{
    use Exporter ();
    use vars	qw ($VERSION @ISA);
    $VERSION    = do { my @r = (q$Revision$ =~ /\d+/g); sprintf "%d."."%02d" x $#r, @r };
    @ISA	= qw (Exporter);
}

use TagX::Term 1.06;

@ISA = qw (TagX::Term);

sub eval
{
    my $self = shift;
    if ( $$self =~ /^\$/ ) {
	$self->resolve_var ($$self, @_);
    } else {
	$$self;
    }
}

1;


#!/usr/local/bin/perl -w

# ident "@(#) $Header$"
# ident "@(#)        $Name$"

package TagX::Header;

use strict;

BEGIN
{
    use Exporter ();
    use vars    qw ($VERSION @ISA);
    $VERSION    = do { my @r = (q$Revision$ =~ /\d+/g); sprintf "%d."."%02d" x $#r, @r };
    @ISA        = qw (Exporter);
}

sub new
{
    my $class = shift;
    my $outhandle = shift;
    bless \$outhandle, $class;
}

sub header
{
    my $self = shift;
    $$self->header(@_);
}

1;


# Apache 2.4/RedHat
APACHE_ROOT=/etc/httpd
APACHE_SITE_ROOT=/var/www
APACHE_MOD_PERL=yes
APACHE_CONF_DIR=/etc/httpd/conf.d
APACHE_VERSION=2.4
BEDROCK_SESSION_DIR=/var/www/session
DEST_DIR=/var/www/bedrock
OWNER=apache
GROUP=apache
BEDROCK_DOCS_ENABLED=On
BEDROCK_FORMS_ENABLED=On
BEDROCK_SESSIONS_ENABLED=On
DBI_HOST=docker_db_1
DBI_USER=fred
DBI_PASS=flintstone
DBI_DB=bedrock

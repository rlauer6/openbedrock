# Apache 2.4/RedHat
APACHE_CONF_DIR=/etc/httpd
APACHE_CONF_INCLUDE_DIR=conf.d
APACHE_LOG_LEVEL=debug
APACHE_MOD_PERL=yes
APACHE_ROOT=/etc/httpd
APACHE_SITE_ROOT=/var/www
APACHE_VERSION=2.4
BEDROCK_BENCHMARK=On
BEDROCK_DOCS_ENABLED=On
BEDROCK_FORMS_ENABLED=On
BEDROCK_SESSIONS_ENABLED=On
BEDROCK_AUTOCOMPLETE_ENABLED=yes
DBI_DB=@mysql_database@
DBI_HOST=@mysql_host@
DBI_PASS=@mysql_password@
DBI_USER=@mysql_user@
GROUP=apache
OWNER=apache

# Apache 2.2/Debian
APACHE_CGI_BIN=/usr/lib/cgi-bin
APACHE_CONF_DIR=/etc/apache2
APACHE_CONF_INCLUDE_DIR=conf-available
APACHE_CONF_SITE_DIR=sites-available
APACHE_LOG_LEVEL=debug
APACHE_MOD_PERL=yes
APACHE_ROOT=/etc/apache2
APACHE_SITE_ROOT=/var/www
APACHE_VERSION=2.2
BEDROCK_DOCS_ENABLED=On
BEDROCK_FORMS_ENABLED=On
BEDROCK_SESSIONS_ENABLED=On
DBI_DB=@mysql_database@
DBI_HOST=@mysql_host@
DBI_PASS=@mysql_password@
DBI_USER=@mysql_user@
GROUP=www-data
OWNER=www-data
